{% extends 'base.html.twig'  %}
{% block titre %}News{% endblock %}
{% block body %}
	
	{% for news  in new %}
			<div class="bloc_page">
				{{ include('includes/titre.html.twig') }}
				
				<div imageCard>
					<img src="{{ asset("uploads/news/" ~ news.picture) }}"  class="fit-image">
				</div>
				<div class="showNew bg-light">
					{{ news.subject }}<br/>
					{{ news }}
				</div>
			</div>
	{% endfor %}
{% endblock %}


